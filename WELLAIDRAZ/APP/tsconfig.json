{
    "compilerOptions": {
      "lib": [
        "dom",
        "dom.iterable",
        "esnext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "strict": false,
      "noEmit": true,
      "incremental": true,
      "module": "esnext",
      "esModuleInterop": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "preserve",
      "paths": {
        "@/*": ["./src/*"]
      },
      "plugins": [
        {
          "name": "next"
        }
      ]
    },
    "include": [
      "next-env.d.ts",
      ".next/types/**/*.ts",
      "**/*.ts",
      "**/*.tsx"
  , "src/app/quizzes/page.jsx", "src/app/createAccount/page.jsx", "src/app/orders/components/ui/EditOrderDash.jsx", "src/app/orders/page.jsx"  ],
    "exclude": [
      "node_modules"
    ]
  }
